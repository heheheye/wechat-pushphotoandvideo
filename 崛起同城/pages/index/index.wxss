.container {
  background: #EDF1FA
}

.top {
  width: 100%;
}

.top image {
  width: 100%;
  height: 400rpx
}

.body1 {
  background: white;
  display: flex;
  justify-content: space-around;
  text-align: center;
  padding: 10rpx 0;
  color: #545454;
  font-size: 30rpx
}

.max {
  width: 120rpx;
  height: 60rpx;
  line-height: 60rpx
}
.body1 image {
  width: 140rpx;
  height: 140rpx
}

.body2 {
  margin-top: 10px;
  background: white
}

.body2-concent {
  display: flex;
  justify-content: space-between;
  padding: 4% 4%;
  border-bottom: 1rpx solid #EAEAEA;
  font-size: 30rpx;
  color: #555555
}

.body2-concent image {
  width: 200rpx;
  height: 200rpx
}

.body2-concent .right {
  margin-left: 40rpx;
}

.right1 {
  padding: 5rpx 0;
  width: 450rpx;
  font-size: 34rpx;
  color: black;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.right2 {
  display: flex;
  height: 85rpx;
  padding: 10rpx 0
}

.text1 {
  width: 20%;
}

.text2 {
  width: 80%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: wrap;
}

.right3 {
  padding: 4rpx 0
}

.more {
  text-align: center
}